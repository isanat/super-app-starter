  "buildCommand": "prisma generate && next build",
  "installCommand": "bun install",
  "framework": "nextjs",
  "regions": ["gru1"],
  "env": {
    "DATABASE_URL": "@database_url",
    "DIRECT_DATABASE_URL": "@direct_database_url",
    "NEXTAUTH_SECRET": "@nextauth_secret",
